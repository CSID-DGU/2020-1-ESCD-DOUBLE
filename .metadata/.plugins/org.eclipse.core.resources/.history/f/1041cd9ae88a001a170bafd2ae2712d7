<mapper namespace="banklist">
<update id="update_Blrest">
	insert into Blist (Bid,Bdate,Brest,Binout,Buser,Bmemo) values(#{Bid},Blist_seq.nextval,,#{Binout},${Buser},#{Bmemo}) 
</update>

</mapper>