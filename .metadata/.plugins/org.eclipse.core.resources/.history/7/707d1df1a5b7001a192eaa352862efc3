<mapper namespace="bank">
<select id="select_bank" resultType="bank">
	select * from bank where Bname=#{Smajor}
</select>
<update id="update_Brest">
	update Bank set Brest=#{Brest} where Bid=#{Bid}
</update>


</mapper>