select * from users;
create table users (
Sid varchar2(20) primary key, 
Sname varchar2(20), 
Spw varchar2(100), 
Snum varchar2(20), 
Sgrade number(2),
Smajor varchar2(20),
Siscouncil number(2) default 0);
drop table users;


create table bank(Bid varchar2(30), Bname varchar2(20),Bpw varchar2(100));

