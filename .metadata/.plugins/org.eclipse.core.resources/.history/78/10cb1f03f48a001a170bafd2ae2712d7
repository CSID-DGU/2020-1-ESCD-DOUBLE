<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC  "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="banklist">
<update id="update_Blrest">
	insert into Blist (Bid,Bdate,Brest,Binout,Buser,Bmemo,Bsequence) values(#{bid},sysdate,#{Brest},#{Binout},#{Buser},#{Bmemo},Bseq.nextval)
</update>
<select id="select_list" resultType="java.util.HashMap">
select * from Blist where Bname=#{s}
</select>
</mapper>